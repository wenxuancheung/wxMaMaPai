/**index.wxss**/
/*轮播图
*/
.swiper image{
  width: 100%;
  height : 100%
}
/*菜单列表*/
.navLists {
  padding-bottom: 20rpx;
  height: 200rpx;
  border-bottom : rgb(245, 245, 245) solid 10rpx;
}
.navIcon{
  float: left;
  text-align: center;
  font-size: 21rpx;
  width: 85rpx;
  height : 85rpx;
  padding-top: 50rpx;
  margin: 0 10rpx 0 120rpx;  
  /* padding-left: 25rpx */
  
}
.navLists view image{
  width :95rpx;
  height: 90rpx
}


/*浮动底部购买*/
.floatBot button{
  position:fixed; 
  width: 90%;
  height: 90rpx;
  border-radius: 80rpx;
  bottom: 5%;
  left: 5%;
  background-color: #FFAA27
}

.floatBot text{
  float: left;
  color : #fff;
  font-size: 32rpx;
  line-height: 90rpx;
  padding: 0 25rpx
}
.floatBot span{
  float: left;
  color : #fff;
  font-size: 28rpx;
  line-height: 90rpx;
  text-decoration: line-through;
}
#floatBotBuy {
  float: right;
  font-size: 28rpx;
  background-color: white;
  width: 30%;
  height: 70rpx;
  margin-top: 10rpx;
  border-radius: 50rpx
}
/*悬浮*/
.toTopArea{
  position: fixed;
  bottom: 20%;
  left: 80%;
}
.to_top button{
  width: 70rpx;
  height: 70rpx;
  border-radius:50%;
  position :relative
}
.to_top text{
  position: absolute;
  top: -8rpx;
  left: 22rpx;
  font-size : 35rpx 
}
.to_top {
  margin: 20rpx 
}
.service button{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  position :relative
}
.service text{
  position: absolute;
  font-size: 25rpx;
  left : 10rpx
}
/*优惠券*/
.getCoupon {
  margin-left : 50rpx
}
.getCouponText {
  font-weight: bold;
  font-size: 50rpx
}
/*领取优惠券*/
.familyCard{
  width : 95%;
  height : 140px;
  margin-top: 50rpx;
  border-bottom: 5rpx #F0F0F0 solid
}
.familyCardImg{
  float: left;
  width: 235rpx;
  height: 235rpx
}
.familyCard .middleText {
  float: left;
  margin-left: 20rpx;
  width: 36%;
}
.mainText{
  width: 100%;
  font-size: 31rpx;
  /*2行文本隐藏*/
  overflow : hidden;
  text-overflow : ellipsis;
  white-space: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.feeText{
  padding: 8rpx 0;
  font-size: 30rpx;
  color:#EA834D
}
.iconText{
  font-size: 30rpx;
  color:#666
}
.iconText image{
  margin-top: 10rpx;
  width: 35rpx;
  height: 35rpx
}
.feeBuy{
  float: right;
  margin-right:13rpx;
}
.quickBuy{
  width: 130rpx;
  height: 70rpx;
  font-size: 28rpx;
  text-align: center;
  background-color: #3ECEA9;
  border-radius: 10rpx
}

/*水平居中*/
.quickBuy text{
  color:#fff;
  line-height: 70rpx
}
.getCouponBuyText{
  font-size: 26rpx;
  color:#6C6C6C
}

/*优选吃喝玩乐*/
.selective{
  border-top: rgb(245, 245, 245) solid 10rpx;
  margin: 0 50rpx;
  height: 100%;
}
.selectiveText{
  float: left;
  display: inline-block;
  margin: 30rpx 0;
  font-size: 50rpx;
  font-weight: bold;

}
.rightMore{
  display: block;
  float: right;
  margin-top : 40rpx;
  font-size: 32rpx;
  color : #969696;
}

/*吃喝玩乐*/
.shopList{
  width: 100%;
  height: 220rpx;
  margin-top: 150rpx;
  border: 5rpx #DFDFDF solid;
  border-radius: 5%;
}
.shopListImg {
  width: 250rpx;
  height: 220rpx;
  float: left;
}
.shopListText {
  font-size: 30rpx;
  float: left
}
.shopListName{
  margin : 10rpx 20rpx;
  font-size: 40rpx;
}
.shopListName image{
  width: 40rpx;
  height: 40rpx
}
.shopListDiscount{
  display: inline-block;
  margin : 5rpx 20rpx;
  font-size: 30rpx;
  color:#DD7C2B;
  border: 1rpx #DD7C2B solid;
  border-radius: 12%;
}
.shopListInfo{
  margin : 5rpx 20rpx;
  font-size: 30rpx;
  color : #909090;
}
/*大吃喝玩乐*/
.playFunView{
  margin-top : 20rpx;
  width: 100%;
  zoom:1;
  overflow: hidden;
}
.playFunInfoView{
  float: left;
  width: 49%;
  height: 500rpx;
  margin-right: 7rpx;
}
.playFunInfoView image{
  height: 300rpx;
  width: 100%;
}
.playFunViewName{
  display: inline-block;
  width: 100%;
  font-size: 32rpx;
  overflow : hidden;
  text-overflow : ellipsis;
  white-space : nowrap;
}
.playFunViewMoney{
  display: inline-block;
  width: 100%;
  font-size: 30rpx;
  margin-right: 10rpx;
  color: #B1844E
}
.playFunViewMoneyDel{
  color : #696969;
  font-size: 25rpx;
  text-decoration: line-through;
}
.playFunViewSale{
  font-size : 25rpx;
  color : #696969;
  display: inline-block;
}

/*活动推荐*/
.activityRecommend{
  border-top: 10rpx solid #F5F5F5;
}
.activityRecommendBody {
  margin: 30rpx 50rpx 0;
}
.activityRecommendText {
  display: inline-block;
  font-size: 50rpx;
  font-weight: bold;
  float: left;
  margin-bottom: 20rpx;
}
.more {
  display: block;
  float: right;
  margin-top: 15rpx;
  font-size: 32rpx;
  color : #969696;
}

.activity{
  width: 100%;
  margin :50rpx 0;
}
.activity image{
  width: 100%;
  height: 350rpx;
}
.activityName{
  font-size: 35rpx;
  font-weight: bold;
  display: block;
  margin: 10rpx 0;
}
.activityMoney{
  font-size: 30rpx;
}
.activityDiscount{
  font-size: 32rpx;
  color: #D68C59;
  border: 1rpx solid #D68C59; 
  border-radius: 10rpx;
  margin-left: 10rpx;
}
.activityDate{
  margin-top: 5rpx;  
  float: right;
  font-size: 28rpx;
  color:#5F5F5F;
}